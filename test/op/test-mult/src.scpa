(* Ce programme vérifie que l'opérateur "*" fonctionne bien *)

program test
    var i,j,k: int
    begin
        i := 8;
        j := 4;
	    k := i*j;
        write i;
        write "\n";
        write j;
        write "\n";
        write k;
        write "\n";
        write "Fin du programme\n";
    end
