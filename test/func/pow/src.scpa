program test
    var i,n: int
    function power(base: int, exp: int) : int
    var i, p: int;
    begin
        i := 0;
        p := 1;
        while i < exp do
        begin
            p := p * base;
            i := i + 1;
        end;
        return p;
    end;
    begin
        read i;
        read n;
        write power(i, n);
    end